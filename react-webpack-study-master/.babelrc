{
  "presets": [
    [
      "latest",
      {
        "loose": true,
        "modules": false
      }
    ],
    "react",
    "es2015",
    "stage-0"
  ],
  // react-hot-loader在--hot基础上做了额外的处理，来保证状态可以存下来
  "plugins": [
    "transform-decorators-legacy",
    "transform-class-properties",
    "transform-object-rest-spread",
    ["import", { "libraryName": "antd", "style": false }]
  ]
}